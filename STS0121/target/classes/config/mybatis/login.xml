<mapper namespace="login">	

	<select id="selectAll"  resultType="net.hb.crud.LoginDTO">
		select * from  login   
	</select>
	
	 <select id="selectone" resultType="net.hb.crud.LoginDTO">
	   select * from  login   where userid=#{userid}
	</select>
</mapper>